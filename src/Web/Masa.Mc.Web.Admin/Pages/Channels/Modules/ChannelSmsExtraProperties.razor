@inherits AdminCompontentBase
<MForm Model="_model" @ref="Form" EnableValidation Class="my-3">
    <MRow Class="row-box">
        <MCol Md="12">
            <STextField @bind-Value="_model.AccessKeyId"
                        Label="@T("SmsChannelOptions.AccessKeyId")"
                        Hint="@T("ChannelOptions.Tips")">
            </STextField>
        </MCol>
        <MCol Md="12">
            <STextField @bind-Value="_model.AccessKeySecret"
                        Label="@T("SmsChannelOptions.AccessKeySecret")"
                        Hint="@T("ChannelOptions.Tips")"
                        Type="@(_accessKeySecretShow ? "text" : "password")"
                        AppendIcon="@(_accessKeySecretShow ? "mdi-eye" : "mdi-eye-off")" OnAppendClick="()=>_accessKeySecretShow = !_accessKeySecretShow">
            </STextField>
        </MCol>
    </MRow>
</MForm>