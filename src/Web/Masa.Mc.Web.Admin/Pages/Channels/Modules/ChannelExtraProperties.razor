@inherits AdminCompontentBase

@if (Type==ChannelType.Email)
{
    <ChannelEmailExtraProperties @ref="_emailExtraPropertiesRef" @bind-Value="Value" />
}
else if (Type==ChannelType.Sms)
{
    <ChannelSmsExtraProperties @ref="_smsExtraPropertiesRef" @bind-Value="Value" />
}
else if (Type==ChannelType.WebsiteMessage)
{
    <ChannelWebsiteMessageExtraProperties @ref="_websiteMessageExtraPropertiesRef" @bind-Value="Value" />
}
